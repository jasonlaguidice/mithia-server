// YURI MAP CONFIGURATION
// ======================

// Public IP addresses for client connections
// These IPs are sent to clients, so they must be externally accessible
map_ip: ${PUBLIC_MAP_IP}
map_port: 2001

loginip: ${PUBLIC_LOGIN_IP}
loginport: 2000

//logfile
map_log: logs/map.log

//database auto-save time, in second
save_time: 60

//dumping unknown packet file
dump_log: logs/map_dump.log

//dumping unknown packet to file
dump_save: 1

//experience rate.
xprate: ${XP_RATE:-1}
//drop rate(for all items)
droprate: ${DROP_RATE:-1}

town: Neutral
town: Koguryo
town: Buya
town: Nagnang
town: Gogoon
town: Vortex

// Map server instance ID (for multiple map servers handling different towns)
ServerId: ${SERVER_ID:-0}

meta: RidableAnimals
meta: CharicInfo0
meta: CharicInfo1
//meta: Collections
//meta: GroupNames
meta: ItemInfo0
meta: ItemInfo1
